{
    "welcomeFile": "purchaseorderapp/webapp/",
    "authenticationMethod": "route",
    "routes": [
        { 
          "source": "^/purchaseorderapp/webapp/(.*)$", 
          "target": "$1",
          "csrfProtection": false,
          "authenticationType": "xsuaa",
          "localDir": "purchaseorderapp/webapp/" 
        }, 
        { 
          "source": "^/(.*)", 
          "csrfProtection": false,
          "authenticationType": "xsuaa",
          "destination": "srv-api" 
        } 
    ]
  }
  